{
  "name": "beat-em-up-world",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:scss": "stylelint src/**/*.scss",
    "lint:scss:fix": "stylelint src/**/*.scss --fix",
    "lint:js": "eslint . --ext .ts,.tsx",
    "lint:js:fix": "npm run lint:js --fix",
    "preview": "vite preview",
    "icons": "svgo -f src/assets/icons --config=svgo.config.cjs -o dist/images/icons && svg2sprite dist/images/icons public/images/sprite.svg && rimraf dist"
  },
  "dependencies": {
    "@redux-devtools/extension": "^3.3.0",
    "@reduxjs/toolkit": "^2.1.0",
    "@types/recharts": "^1.8.29",
    "@vercel/analytics": "^1.1.2",
    "axios": "^1.7.4",
    "classnames": "^2.5.1",
    "date-fns": "^3.3.1",
    "disqus-react": "^1.1.5",
    "lodash": "^4.17.21",
    "ramda": "^0.29.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-grid-system": "^8.1.9",
    "react-popper-tooltip": "^4.4.2",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.0",
    "recharts": "^2.11.0",
    "redux": "^5.0.1",
    "reselect": "^5.1.0",
    "sass": "^1.70.0",
    "svg2sprite": "^2.0.1",
    "svg2sprite-cli": "^2.0.1",
    "svgo": "^3.2.0",
    "swiper": "^11.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest": "^29.7.0",
    "typescript": "^5.2.2",
    "vite": "^5.4.10"
  }
}

    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "0.9.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.6.4",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "^4.0.1",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-custom-scrollbars-2": "^4.5.0",
    "react-dev-utils": "^10.2.1",
    "react-dom": "^16.13.1",
    "react-grid-system": "^8.1.5",
    "react-popper-tooltip": "^2.11.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.5",
    "react-svg-sprite": "^1.0.8",
    "redux-devtools-extension": "^2.13.8",
    "reselect": "^4.1.7",
    "resolve": "^1.15.0",
    "resolve-url-loader": "3.1.1",
    "sass": "^1.45.1",
    "sass-loader": "8.0.2",
    "semver": "7.5.2",
    "style-loader": "0.23.1",
    "swiper": "^8.4.5",
    "terser-webpack-plugin": "2.3.5",
    "ts-pnp": "1.1.6",
    "typescript": "^4.9.4",
    "url-loader": "2.3.0",
    "webpack": "4.42.0",
    "webpack-dev-server": "3.10.3",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "4.3.1"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "lint:scss": "stylelint src/**/*.scss",
    "lint:scss:fix": "stylelint src/**/*.scss --fix",
    "lint:js": "eslint . --ext .ts,.tsx",
    "lint:js:fix": "npm run lint:js --fix",
    "icons": "svgo -f src/assets/icons --config=svgo-config.yml -o dist/images/icons && svg2sprite dist/images/icons public/images/sprite.svg && rimraf dist"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": "eslint --fix",
    "src/**/*.scss": "stylelint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "^16.18.8",
    "@types/power-assert": "^1.5.3",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint": "^7.1.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "postcss": "^8.4.5",
    "power-assert": "^1.6.1",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "ramda": "^0.27.1",
    "react-test-renderer": "^17.0.1",
    "recharts": "^2.1.9",
    "rimraf": "^3.0.2",
    "stylelint": "^14.2.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^4.1.0",
    "svg-inline-loader": "^0.8.2",
    "svg2sprite": "^2.0.1",
    "svg2sprite-cli": "^2.0.1",
    "svgo": "^1.3.2",
    "ts-jest": "^26.5.4"
  }
}
